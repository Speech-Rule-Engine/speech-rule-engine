#!/usr/bin/env node
require('../lib/app.js');
let promise = Sre.setupEngine({}).then(() => Sre.enginePromise());
promise.then(async () => {
  Sre.setupEngine({locale: 'en'});
  return Sre.enginePromise();
}).then(() => {
  let stree = Sre.getTreeFromString('<math><mo>=</mo></math>');
  let result = Sre.EngineInstance().evaluateNode(stree);
});

