#!/usr/bin/env node
require('../lib/app.js');


async function run2() {
  await Sre.setupEngine({}).
    then(() => Sre.enginePromise()).
    then(() => Sre.setupEngine({locale: 'en'})).
    then(() => {
      let stree = Sre.xmlTreeFromString('<math><mo>=</mo></math>');
      let result = Sre.EngineInstance().evaluateNode(stree);
      console.log(result);
    });
}

run2();
