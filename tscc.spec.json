{
  "modules": {
    "bundle": "ts/xplatmath/xplatmath.ts"
  },
  "debug": {
    "persistArtifacts": false
  },
  "compilerFlags": {
    "assume_function_wrapper": true,
    "rewrite_polyfills": true,
    "language_in": "UNSTABLE",
    "language_out": "ECMASCRIPT_NEXT",
    "variable_renaming_report": "xplatmath/report.map",
    "js_output_file": "xplatmath/app.js",
    "externs": [
      "ts/common/externs.js",
      "ts/externs.js",
      "ts/rule_engine/externs.js",
      "ts/l10n/externs.js",
      "ts/audio/externs.js"
    ]
  }
}
