{
  "locale": "af",
  "modality": "speech",
  "domain": "clearspeak",
  "inherits": "base",
  "rules": [
    [
      "Precondition",
      "font-double-struck",
      "default",
      "self::*[@font=\"double-struck\"]",
      "string-length(text())=1",
      "not(contains(@grammar, \"ignoreFont\"))"
    ]
  ]
}
