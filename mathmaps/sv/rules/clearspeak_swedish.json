{
  "domain": "clearspeak",
  "locale": "sv",
  "modality": "speech",
  "inherits": "base",
  "rules": [
    [
      "Precondition",
      "font-double-struck",
      "default",
      "self::*[@font=\"double-struck\"]",
      "string-length(text())=1",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Specialized",
      "font-double-struck",
      "default",
      "Caps_SayCaps"
    ],
    [
      "Precondition",
      "german-font",
      "default",
      "self::*[@font=\"fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "german-font-bold",
      "default",
      "self::*[@font=\"bold-fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "unit-square",
      "default",
      "self::superscript[@role=\"unit\"]",
      "children/*[2][text()=2]",
      "name(children/*[1])=\"identifier\"",
      "children/*[1][@category=\"unit:length\"]"
    ],
    [
      "Precondition",
      "unit-cubic",
      "default",
      "self::superscript[@role=\"unit\"]",
      "children/*[2][text()=3]",
      "name(children/*[1])=\"identifier\"",
      "children/*[1][@category=\"unit:length\"]"
    ]
  ]
}
