{
  "domain": "mathspeak",
  "locale": "ko",
  "modality": "speech",
  "inherits": "base",
  "rules": [
    [
      "Ignore",
      "fraction"
    ],
    [
      "Ignore",
      "fraction-brief"
    ],
    [
      "Ignore",
      "fraction-sbrief"
    ],
    [
      "Ignore",
      "vulgar-fraction"
    ],
    [
      "Ignore",
      "root-small"
    ],
    [
      "Ignore",
      "root-small-brief"
    ],
    [
      "Ignore",
      "root-small-sbrief"
    ],
    [
      "Ignore",
      "root"
    ],
    [
      "Ignore",
      "root-brief"
    ],
    [
      "Ignore",
      "root-sbrief"
    ],
    [
      "Precondition",
      "german-font",
      "default",
      "self::*[@font=\"fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "german-font-bold",
      "default",
      "self::*[@font=\"bold-fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "quad",
      "default",
      "self::superscript",
      "children/*[2]",
      "children/*[2][text()=4]",
      "name(children/*[1])!=\"text\" or not(name(children/*[1])=\"text\" and (name(../../../punctuated[@role=\"text\"]/..)=\"stree\" or name(..)=\"stree\"))",
      "name(children/*[1])!=\"subscript\" or (name(children/*[1])=\"subscript\" and name(children/*[1]/children/*[1])=\"identifier\" and name(children/*[1]/children/*[2])=\"number\" and children/*[1]/children/*[2][@role!=\"mixed\"] and children/*[1]/children/*[2][@role!=\"othernumber\"])",
      "not(@embellished)"
    ],
    [
      "Specialized",
      "quad",
      "default",
      "brief"
    ],
    [
      "Specialized",
      "quad",
      "default",
      "sbrief"
    ],
    [
      "Alias",
      "quad",
      "self::superscript",
      "children/*[2]",
      "children/*[2][text()=4]",
      "@embellished",
      "children/*[1][@role=\"prefix operator\"]"
    ],
    [
      "Precondition",
      "root-simple",
      "default",
      "self::root",
      "CQFisSimpleIndex"
    ],
    [
      "Precondition",
      "root-simple-brief",
      "brief",
      "self::root",
      "CQFisSimpleIndex"
    ],
    [
      "Precondition",
      "root-simple-sbrief",
      "sbrief",
      "self::root",
      "CQFisSimpleIndex"
    ],
    [
      "Precondition",
      "currency",
      "default",
      "self::infixop",
      "children/*[1][@role=\"unit\"]",
      "children/*[1][@category=\"unit:currency\"]"
    ],
    [
      "Precondition",
      "greater-equal",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "content/*[1]=\"≥\""
    ],
    [
      "Precondition",
      "smaller-equal",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"≤\"'"
    ],
    [
      "Precondition",
      "greater",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\">\""
    ],
    [
      "Precondition",
      "smaller",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"<\""
    ],
    [
      "Precondition",
      "set-member",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"∈\" or text()=\"∊\""
    ],
    [
      "Precondition",
      "contain-member",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"∋\" or text()=\"∍\"" 
    ],
    [
      "Precondition",
      "subset-of",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"⊂\""
    ],
    [
      "Precondition",
      "not-equal-to",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"≠\""
    ],
    [
      "Precondition",
      "not-set-member",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"∉\""
    ],
    [
      "Precondition",
      "not-contain-member",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"∌\""
    ],
    [
      "Precondition",
      "parallel-to",
      "default",
      "self::punctuated",
      "count(./children/*)=3",
      "content/*[1][text()]=\"∥\""
    ],
    [
      "Precondition",
      "vertical",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"⊥\""
    ],
    [
      "Precondition",
      "necessary-condition",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"⇐\""
    ],
    [
      "Precondition",
      "sufficient-condition",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"⇒\""
    ],
    [
      "Precondition",
      "necessary-sufficient-condition",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"⇔\""
    ],
    [
      "Precondition",
      "line-segment",
      "default",
      "self::overscore",
      "@role=\"implicit\"",
      "children/*[2][@role=\"overaccent\"]",
      "children/*[2][contains(@annotation, \"accent:bar\")]",
      "name(children/*[1])=\"infixop\"",
      "count(./children/*[1]/children/*)=2"
    ]
  ]
}
