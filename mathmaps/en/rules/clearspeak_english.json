{
  "domain": "clearspeak",
  "locale": "en",
  "modality": "speech",
  "inherits": "base",
  "rules": [
    [
      "Precondition",
      "german-font",
      "default",
      "self::*[@font=\"fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "german-font-bold",
      "default",
      "self::*[@font=\"bold-fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "unit-square",
      "default",
      "self::superscript[@role=\"unit\"]",
      "children/*[2][text()=2]",
      "name(children/*[1])=\"identifier\"",
      "children/*[1][@category=\"unit:length\"]"
    ],
    [
      "Precondition",
      "unit-cubic",
      "default",
      "self::superscript[@role=\"unit\"]",
      "children/*[2][text()=3]",
      "name(children/*[1])=\"identifier\"",
      "children/*[1][@category=\"unit:length\"]"
    ],
    [
      "Rule",
      "Unit-normal-test",
      "default",
      "[t] text() (grammar:annotation=\"unit\":translate)",
      "self::text",
      "@font=\"normal\""
    ],
    [
      "Rule",
      "limit-new",
      "default",
      "[n] children/*[1] (grammar:addArticle); [t] \"as\"; [n] children/*[2]/children/*[1]; [t] \"approaches\"; [n] children/*[2]/children/*[2]",
      "self::limlower[@role=\"limit function\"]"
    ],
    [
      "Rule",
      "font-new",
      "default",
      "[n] self::* (grammar:ignoreFont=@font)",
      "self::identifier[@font=\"normal\"]",
      "@role=\"unknown\"",
      "string-length(text())=1",
      "not(contains(@grammar, \"ignoreFont\"))"
    ]
  ]
}
