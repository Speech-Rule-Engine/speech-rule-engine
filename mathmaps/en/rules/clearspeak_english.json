{
  "domain": "clearspeak",
  "locale": "en",
  "modality": "speech",
  "inherits": "base",
  "rules": [
    [
      "Precondition",
      "german-font",
      "default",
      "self::*[@font=\"fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "german-font-bold",
      "default",
      "self::*[@font=\"bold-fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ]
  ]
}
